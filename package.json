{
  "name": "map-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chroma-js": "^1.3.4",
    "concurrently": "^3.5.0",
    "d3-dsv": "^1.0.7",
    "d3-geo": "^1.9.0",
    "d3-interpolate": "^1.1.5",
    "d3-scale": "^1.0.6",
    "material-ui": "^1.0.0-beta.19",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-scripts": "1.1.4",
    "styled-components": "^3.2.5",
    "topojson-client": "^3.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "watch-sass": "node-sass src/style.scss src/style.css -w --source-map=src/style.css.map",
    "start-js": "react-scripts start",
    "start": "concurrently -c \"magenta,cyan\" --names \"Sass,React\" \"npm run watch-sass\" \"npm run start-js\"",
    "build-sass": "node-sass src/style.scss src/style.css --output-style=compressed",
    "build-js": "react-scripts build",
    "build": "npm run build-sass && npm run build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.12.0",
    "redux-devtools-extension": "^2.13.2"
  }
}
