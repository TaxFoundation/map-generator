{
  "name": "map-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "chroma-js": "^2.0.2",
    "concurrently": "^4.1.0",
    "d3-dsv": "^1.0.10",
    "d3-format": "^1.3.2",
    "d3-geo": "^1.11.3",
    "d3-interpolate": "^1.3.2",
    "d3-scale": "^2.2.2",
    "eslint-plugin-react-hooks": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-scripts": "2.1.3",
    "styled-components": "^4.1.3",
    "topojson-client": "^3.0.0"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "watch-sass": "node-sass src/style.scss src/style.css -w --source-map=src/style.css.map",
    "start-js": "react-scripts start",
    "start": "concurrently -c \"magenta,cyan\" --names \"Sass,React\" \"npm run watch-sass\" \"npm run start-js\"",
    "build-sass": "node-sass src/style.scss src/style.css --output-style=compressed",
    "build-js": "react-scripts build",
    "build": "npm run build-sass && npm run build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
